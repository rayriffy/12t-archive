<script type="text/javascript">
      var randomImage = {
        paths: [
          /*"/images/character.png",
          "/images/character2.png",
		  "/images/character3.png",
		  "/images/character4.png"*/

		  "http://i.imgur.com/a5oROSw.png",
          "http://i.imgur.com/VxNOTCq.png",
		  "http://i.imgur.com/69ex0x3.png",
		  "http://i.imgur.com/yxTQW4g.png"
        ],
        
        generate: function(){
          var path = randomImage.paths[Math.floor(Math.random()*randomImage.paths.length)];
          var img = new Image();
          img.src = path;
          img.alt = "12Tails 12หางออนไลน์"; 
          $("#randomImage").html(img);
          $("#randomImage").attr(path);
        }
      }
      randomImage.generate();
</script> 
